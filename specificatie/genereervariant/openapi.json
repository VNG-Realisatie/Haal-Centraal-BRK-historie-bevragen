          "beperkingsgebied" : {
            "$ref" : "#/components/schemas/Beperkingsgebied"
          }
        },
        "description" : "= PubliekrechtelijkeBeperking in BRK bevragen minus\n  - isGebaseerdOpStukdeelIdentificatie\n  - isVermeldInStukdeelIdentificaties\n"
      },
      "Waardelijst" : {
        "type" : "object",
        "properties" : {
          "code" : {
            "minLength" : 1,
            "type" : "string"
          },
          "waarde" : {
            "type" : "string"
          }
        }
      },
      "NietNatuurlijkPersoonBeperkt" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/PersoonBasis"
        }, {
          "properties" : {
            "type" : {
              "$ref" : "#/components/schemas/NietNatuurlijkPersoonType_enum"
            }
          },
          "description" : "Een niet-natuurlijk persoon."
        } ]
      },
      "PersoonBasis" : {
        "type" : "object",
        "properties" : {
          "identificatie" : {
            "type" : "string",
