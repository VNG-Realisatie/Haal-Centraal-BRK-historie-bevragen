          "bedragTransactiesomLevering" : {
            "$ref" : "#/components/schemas/Bedrag"
          },
          "bedragVorderingsbeslag" : {
            "$ref" : "#/components/schemas/Bedrag"
          },
          "bedragZekerheidsstellingHypotheek" : {
            "$ref" : "#/components/schemas/Bedrag"
          },
          "omschrijvingGekozenWoonplaats" : {
            "type" : "string"
          },
          "omschrijvingKadastraleObjecten" : {
            "type" : "string"
          },
          "omschrijvingTopografischeMutatie" : {
            "type" : "string"
          },
          "redenDoorhalingTeboekstelling" : {
            "$ref" : "#/components/schemas/Waardelijst"
          }
        },
        "description" : "Een stukdeel is een paragraaf in een akte waarmee een recht gevestigd wordt."
      },
      "Bedrag" : {
        "type" : "object",
        "properties" : {
          "som" : {
            "type" : "number",
            "description" : "Het bedrag. Dit is een waarde met max. 2 decimalen.",
            "format" : "float"
          },
          "valuta" : {
            "$ref" : "#/components/schemas/Waardelijst"
          }
        },
        "description" : "Hoeveelheid geld in cijfers in een bepaalde valuta.\n\nWaardelijst in deze component : [Valuta](http://www.kadaster.nl/schemas/waardelijsten/Valuta/)"
      },
      "Beperkingsgebied_werkingsgebied" : {
        "type" : "object",
        "properties" : {
          "bgtObjectIdentificatie" : {
            "type" : "string"
          },
          "bagObjectIdentificatie" : {
            "type" : "string"
          },
          "kadastraalObjectIdentificatie" : {
            "type" : "string"
          },
          "vrijeContour" : {
            "$ref" : "#/components/schemas/polygonGeoJSON"
          }
        },
        "description" : "Datgene waarop de beperking van toepassing is."
      }
    }
