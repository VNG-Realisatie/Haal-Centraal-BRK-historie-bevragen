          },
          "voorkomen" : {
            "$ref" : "#/components/schemas/PubliekrechtelijkeBeperkingVoorkomen"
          }
        }
      },
      "Geldig" : {
        "type" : "object",
        "properties" : {
          "van" : {
            "type" : "string",
            "format" : "date"
          },
          "totEnMet" : {
            "type" : "string",
            "format" : "date"
          }
        }
      },
      "PubliekrechtelijkeBeperkingVoorkomen" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/PubliekrechtelijkeBeperking"
        }, {
          "properties" : {
            "isGebaseerdOpStukdeel" : {
              "$ref" : "#/components/schemas/Stukdeel"
            },
            "isVermeldInStukdelen" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/components/schemas/Stukdeel"
              }
            }
          }
        } ]
      },
      "PubliekrechtelijkeBeperking" : {
        "type" : "object",
        "properties" : {
          "identificatie" : {
            "type" : "string"
          },
          "domein" : {
            "type" : "string",
            "description" : "Het domein waartoe de identificatie behoort."
          },
          "grondslag" : {
            "$ref" : "#/components/schemas/Waardelijst"
          },
          "datumInWerking" : {
            "type" : "string",
            "format" : "date"
          },
          "datumBeeindiging" : {
            "type" : "string",
            "format" : "date"
          },
          "bevoegdGezag" : {
            "$ref" : "#/components/schemas/NietNatuurlijkPersoonBeperkt"
